# Test skipping a test

! gno test -v .

! stdout .+
stderr 'Hey'
stderr 'I.m on strike!'
! stderr 'this should not print'
stderr '--- SKIP: TestSkip'

-- skip.gno --
package skip

-- skip_test.gno --
package skip

import "testing"

func TestSkip(t *testing.T) {
	t.Log("Hey")
	t.Skip("I'm on strike!")
	t.Log("this shouldn't print")
}