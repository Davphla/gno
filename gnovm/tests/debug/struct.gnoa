package main

import (
	"errors"
	"fmt"
)

type Foo struct {
	N int
}

func main() {
	err1 := errors.New("xxx")
	err2 := errors.New("xxx")

	println(err1 == err2)

	s1 := &Foo{N: 1}
	s2 := &Foo{N: 1}

	fmt.Println(s1)
	fmt.Println(s2)

	println(s1 == s2)

	s3 := Foo{N: 1}
	s4 := Foo{N: 1}

	fmt.Println(s3)
	fmt.Println(s4)

	println(s3 == s4)

}

// Output:
// false
// &{1}
// &{1}
// false
// {1}
// {1}
// true
